(define instance (:: Webgraphe\Phlip\Tests\Dummy fromScalars "Hello" 42 3.1415926536 (= 1 1)))
(assert-true (instance? instance Webgraphe\Phlip\Tests\Dummy))
(assert-equals "Hello" (-> instance string))
(assert-equals 42 (-> instance int))
(assert-equals 3.1415926536 (-> instance float))
(assert-equals (= 1 1) (-> instance bool))
(assert-equals "public instance method" (-> instance publicInstanceMethod))
(assert-equals "same instance name method" (-> instance sameInstanceName))
(assert-equals "same instance name field" (-> instance -sameInstanceName))
(assert-equals "World" (set! (-> instance string) "World"))
(assert-equals "World" (-> instance string))
