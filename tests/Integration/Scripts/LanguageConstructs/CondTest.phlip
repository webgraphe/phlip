(define (month index)
    (cond
        ((= index 1) "January")
        ((= index 2) "February")
        ((= index 3) "March")
        ((= index 4) "April")
        ((= index 5) "May")
        ((= index 6) "June")
        ((= index 7) "July")
        ((= index 8) "August")
        ((= index 9) "September")
        ((= index 10) "October")
        ((= index 11) "November")
        ((= index 12) "December")
    )
)

(assert-equals null (month 0))
(assert-equals "January" (month 1))
(assert-equals "February" (month 2))
(assert-equals "March" (month 3))
(assert-equals "April" (month 4))
(assert-equals "May" (month 5))
(assert-equals "June" (month 6))
(assert-equals "July" (month 7))
(assert-equals "August" (month 8))
(assert-equals "September" (month 9))
(assert-equals "October" (month 10))
(assert-equals "November" (month 11))
(assert-equals "December" (month 12))
(assert-equals null (month 13))
