(test "Logical tests"
    (assert (= 4 (and 1 2 3 4)))
    (assert (= 0 (and 1 2 3 -1 0)))
    (assert (= 123 (or null 0 "" 123)))
    (assert (= 1 (if (not true) 0 1)))
    (assert (= 0 (if (not false) 0 1)))
    (assert (= true true))
    (assert (= false false))
    (assert (= null null))
    (assert (equals? 'true 'true))
    (assert (equals? ''true ''true))
    (assert (xor true false))
    (assert (xor false true))
    (assert (not (xor true true)))
    (assert (not (xor false false)))
)

(test "Logical comparison of lists"
    (assert-exception "RuntimeException" (= 'false 'false))
)